// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/.DS_Store": true,
      "**/.fake": true
    },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/packages": true,
    "**/paket-files": true
  },
  "FSharp.excludeProjectDirectories": [
    ".git",
    "paket-files",
    "release"
  ],
  "workbench.colorCustomizations": {
    "tab.unfocusedActiveBorder": "#fff0"
  },
  "FSharp.trace.server": {
    "format": "json",
    "verbosity": "verbose",
    "port": 88769
  }

}
